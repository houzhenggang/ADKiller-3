{
	"manifest_version":2,
	"name":"cnblog Message",
	"version":"1.0.0",
	"background":{"scripts":["jquery-2.0.0.min.js","background.js"]},
	"page_action":{
		"default_icon":{
	 		"19":"cnblogs_19.png",
	 		"38":"cnblogs_38.png",
		},
		"default_title":"cnblog page message",
		"default_popup:"popup.html"
	
	},
	"permissions":{"tabs","http://localhost/"},
	"content_scripts":[{
		"matches":["http://www.cnblogs.com/*"],
		"js":["content_script.js","jquery-2.0.0.min.js"],
	}]

}
