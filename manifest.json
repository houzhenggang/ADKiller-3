{
	"manifest_version":2,
	"name":"广告杀手",
	"version":"1.0.0",
	"background":{"scripts":["CounterMgr.js","BlockUrlMgr.js","background.js"]},
	"browser_action": {
    "default_icon": "cnblogs_19.png",  
    "default_title": "广告杀手",       
    "default_popup": "popup.html"        
    },
	"permissions":["contextMenus","http://*/*","webRequest","webRequestBlocking","tabs"],
	"content_scripts":[{
		"matches":["http://*/*"],
		"js":["jquery-2.0.0.min.js","content_script.js"]
	}],
	"options_page": "options.html",
	"web_accessible_resources":["jquery.min.map"]
}
